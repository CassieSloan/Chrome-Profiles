{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "128": "assets/logo-128.png",
         "16": "assets/logo-16.png",
         "48": "assets/logo-48.png"
      },
      "default_popup": "popup.html",
      "default_title": "MarkUp"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "markup-content.js" ],
      "matches": [ "https://app.markup.io/*" ],
      "run_at": "document_start"
   } ],
   "description": "Visual Feedback for Websites",
   "icons": {
      "128": "assets/logo-128.png",
      "16": "assets/logo-16.png",
      "48": "assets/logo-48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "MarkUp For Chrome",
   "permissions": [ "tabs", "webNavigation", "webRequest", "webRequestBlocking", "storage", "alarms", "cookies", "\u003Call_urls>" ],
   "short_name": "MarkUp.io",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.2.0"
}
