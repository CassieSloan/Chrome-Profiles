{
   "author": "Level Access",
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "128": "img/access-assistant-128.png",
         "16": "img/access-assistant-16.png",
         "32": "img/access-assistant-32.png",
         "48": "img/access-assistant-48.png"
      },
      "default_title": "Access Assistant"
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "default": "Ctrl+Shift+Space",
            "mac": "MacCtrl+Shift+Space",
            "windows": "Ctrl+Shift+Space"
         }
      },
      "freeze-thaw": {
         "description": "Freeze/Thaw Page-under-test",
         "suggested_key": {
            "default": "Ctrl+Shift+F",
            "mac": "MacCtrl+Shift+F",
            "windows": "Ctrl+Shift+F"
         }
      }
   },
   "description": "Access Assistant",
   "icons": {
      "128": "img/access-assistant-128.png",
      "16": "img/access-assistant-16.png",
      "32": "img/access-assistant-32.png",
      "48": "img/access-assistant-48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Access Assistant",
   "optional_permissions": [ "http://*/*", "https://*/*", "file://*/*" ],
   "permissions": [ "activeTab", "tabs" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "8.10.0.11",
   "web_accessible_resources": [ "app.js", "AccessEngine.professional.js", "LevelAccess-AST.js", "LevelAccess-Macro.js", "LevelAccess-AST.css", "ast.css", "astWindow.js", "img/*", "fonts/**/*" ]
}
